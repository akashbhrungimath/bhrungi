#include<stdio.h>
#include<string.h>
struct employee
{
    char eid[20];
    char n[20];
    char doj[20];
    double s;
};
int main()
{
    struct employee e1;
    printf("enter the ID of the employee\n");
    gets(e1.eid);
    printf("enter the name of the employee\n");
    gets(e1.n);
    printf("enter the date of joining of the employee\n");
    gets(e1.doj);
    printf("enter the salary of the employee\n");
    scanf("%lf",&e1.s);
    printf("details of the employee above entered is \n");
    printf("\nID:  ");
    puts(e1.eid);
    printf("\nName of the employee: ");
    puts(e1.n);
    printf("\nDate of the employee is: ");
    puts(e1.doj);
    printf("\nSalary of the employee: %lf\n\n",e1.s);
    return 0;
}
    