#include<stdio.h>
void swap(int *,int *);
int main()
{
    int a,b;
    printf("enter the two numbers to swap it\na=");
    scanf("%d",&a);
    printf("b=");
    scanf("%d",&b);
    swap(&a,&b);
    printf("swaping result is\na=%d\nb=%d\n",a,b);
    return 0;
}
void swap(int *a,int *b)
{
    int t;
    t=*a;
    *a=*b;
    *b=t;
}
    