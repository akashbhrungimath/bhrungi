#include<stdio.h>
#include<math.h>
int main()
{
   float x1,x2,y1,y2,d;
   printf("enter the value of x1,x2,y1,y2 to find the distance between the points in 2D\n");
   scanf("%f%f%f%f",&x1,&x2,&y1,&y2);
   d=sqrt(pow((x2-x1),2)+pow((y2-y1),2));
   printf("distance between the entered points is=%f\n",d);
   return 0;
}