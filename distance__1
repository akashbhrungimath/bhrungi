#include <stdio.h>
#include <math.h>
int main()
{
   float x1,y1,x2,y2,d;
   printf("enter the co-ordinates of two points to find the distance between them\n");
   scanf("%f%f%f%f",&x1,&y1,&x2,&y2);
   d=sqrt(pow((x2-x1),2)+pow((y2-y1),2));
   printf("distance between the entered points is=%f\n",d);
   return 0;
}