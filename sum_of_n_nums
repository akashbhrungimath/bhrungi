#include <stdio.h>
int main()
{
    int n,a[n],i,sum=0;
    printf("enter the number of numbers you want to enter=");
    scanf("%d",&n);
    printf("enter the numbers=\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        sum=sum+a[i];
    }
    printf("the sum of all numbers is=%d\n",sum);
    return 0;
}