#include <stdio.h>
int main()
{
    int n,i;
    float a[n],sum=0;
    printf("enter the number of numbers you want to enter=");
    scanf("%d",&n);
    printf("enter the numbers=\n");
    for(i=0;i<n;i++)
    {
        scanf("%f",&a[i]);
        sum=sum+a[i];
    }
    printf("the sum of all numbers is=%f\n",sum);
    return 0;
}